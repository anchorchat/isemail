{
  "name": "@anchorchat/isemail",
  "description": "Validate an email address according to RFCs 5321, 5322, and others",
  "version": "3.0.0",
  "repository": "git://github.com/anchorchat/isemail",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "jsnext:main": "./es/index.js",
  "keywords": [
    "isemail",
    "validation",
    "check",
    "checking",
    "verification",
    "email",
    "address",
    "email address"
  ],
  "files": [
    "es",
    "lib",
    "src"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "punycode": "2.x.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "code": "3.x.x",
    "cross-env": "^5.1.3",
    "lab": "13.x.x",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:commonjs && npm run build:es",
    "clean": "rimraf es lib",
    "prepare": "npm run clean && npm run build",
    "lint": "lab -dL",
    "test": "lab -a code -t 100 -m 5000",
    "test-cov-html": "lab -a code -r html -o coverage.html -m 5000"
  },
  "license": "BSD-3-Clause"
}
